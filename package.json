{
  "name": "module-md",
  "version": "0.90.0",
  "description": "Module MD - markdown & html documentation generator",
  "keywords": [
    "module",
    "markdown",
    "html",
    "api",
    "doc",
    "docs",
    "documentation"
  ],
  "author": {
    "name": "Malte-Thorben Bruns",
    "email": "skenqbx@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/skenqbx/module-md.git"
  },
  "private": true,
  "dependencies": {
    "ast-walker": "git+https://github.com/skenqbx/ast-walker.git#master",
    "espree": "2.0.0",
    "file-emitter": "git+https://github.com/skenqbx/file-emitter.git#master",
    "handlebars": "3.0.1",
    "read-package-json": "2.0.0",
    "read-package-tree": "4.0.1"
  },
  "optionalDependencies": {},
  "devDependencies": {
    "eslint": "0.18.0",
    "istanbul": "0.3.13",
    "mocha": "2.2.4",
    "next-update": "0.7.4"
  },
  "scripts": {
    "lint": "node_modules/.bin/eslint ./",
    "test": "node_modules/.bin/eslint ./ && node_modules/.bin/istanbul cover node_modules/.bin/_mocha -- ./test",
    "update": "node_modules/.bin/next-update -k",
    "clean": "rm -rf node_modules/ && rm -rf coverage/ && rm npm-debug.log"
  },
  "engines": {
    "node": ">=0.10",
    "npm": "~2.7.5"
  }
}
